<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <welcome-file-list>
        <welcome-file>/TemplateWeb/index.jsp</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>ReporteVictimaServlet</servlet-name>
        <servlet-class>org.cfg.uapa.java.sisdevi.controller.ReporteVictimaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReporteTestigoServlet</servlet-name>
        <servlet-class>org.cfg.uapa.java.sisdevi.controller.ReporteTestigoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ReporteVictimaServlet</servlet-name>
        <url-pattern>/ReporteVictimaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ReporteTestigoServlet</servlet-name>
        <url-pattern>/ReporteTestigoServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
